<%- include('header') -%>
<div class="container main">
  <h1>This is the main page</h1>
</div>


<% blogs.forEach(function(blog){ %>
  <div class="">
    <h4><%= blog.title%></h4>
    <img src="<%=blog.image%>" alt="">
    <span><%=blog.created%></span>
    <p><%=blog.body%></p>
    <a href="/blogs/<%=blog._id%>">Read More</a>
  </div>

<%})%>

<%- include('footer') -%>
